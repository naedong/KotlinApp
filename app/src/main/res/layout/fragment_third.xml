<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ThirdFragment">

    <!-- TODO: Update blank fragment layout -->


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.widget.Button
                android:id="@+id/btn_start"
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="시작하기"
                android:textColor="@color/mainWhite"
                android:textSize="20sp"

                android:fontFamily="@font/imcresoojin"
                android:background="@drawable/dimension_button"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"></android.widget.Button>


            <android.widget.Button
                android:id="@+id/btn_regi"
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:text="등록하기"
                android:textColor="@color/mainWhite"
                android:textSize="20sp"

                android:fontFamily="@font/imcresoojin"
                android:background="@drawable/dimension_button"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_start"
                app:layout_constraintVertical_bias="0.17000002"></android.widget.Button>

            <android.widget.Button
                android:id="@+id/btn_using"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:text="사용법"
                android:textSize="20sp"
                android:fontFamily="@font/imcresoojin"
                android:background="@drawable/dimension_button_test"
                   app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"></android.widget.Button>


        </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>